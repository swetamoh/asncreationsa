<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Popover showHeader="false" id="detailPopover" class="sapUiContentPadding" placement="Top">
		<Table id="DeliveryTableId" class="sapUiSizeCompact" items="{materialDescModel>/}" itemPress="onItempress" fixedLayout="false">
			<columns>
				<Column id="_IDGenColumn1" minScreenWidth="Tablet" demandPopin="true">
					<Text id="_IDGenText1" text="Material"/>
				</Column>
				<Column id="_IDGenColumn2" minScreenWidth="Tablet" demandPopin="true">
					<Text id="_IDGenText2" text="Description"/>
				</Column>
				<Column id="_IDGenColumn3" minScreenWidth="Tablet" demandPopin="true">
					<Text id="_IDGenText3" text="Quantity"/>
				</Column>
				<Column id="_IDGenColumn4" minScreenWidth="Tablet" demandPopin="true">
					<Text id="_IDGenText4" text="Unit"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem id="TableTempId">
					<cells>
						<Text id="_IDGenText5" text="{materialDescModel>ItemCode}"/>
						<Text id="_IDGenText6" text="{materialDescModel>ItemDesc}"/>
						<Text id="_IDGenText7" text="{materialDescModel>PoQty}"/>
						<Text id="_IDGenText8" text="{materialDescModel>UnitPrice}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Popover>
</core:FragmentDefinition>